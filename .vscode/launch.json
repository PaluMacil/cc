    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch CC",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${workspaceRoot}/cmd/cc",
            "env": {
                "CCUTIL_CONFIG_PATH": "../../config.json",
                "CCUTIL_BIN_PATH": "../../dist/bin"
            },
            "args": [""],
            "showLog": true
        },
        {
            "name": "Launch CCUTIL",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${workspaceRoot}/cmd/ccutil",
            "env": {
                "CCUTIL_CONFIG_PATH": "../../config.json",
                "CCUTIL_BIN_PATH": "../../dist/bin"
            },
            "args": ["remove", "TDM"],
            "showLog": true
        }
    ]
}